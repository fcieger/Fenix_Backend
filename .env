# Configurações do Backend FENIX
# Renomeie este arquivo para .env

NODE_ENV=development
PORT=3001

# Configurações do Banco de Dados
DATABASE_URL=postgresql://postgres:fenix123@localhost:5432/fenix
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=fenix123
DB_DATABASE=fenix

# Configurações JWT
JWT_SECRET=fenix-jwt-secret-key-2024-super-secure
JWT_EXPIRES_IN=24h

# Configurações CORS
CORS_ORIGIN=http://localhost:3004,https://fenixfrontendatual.vercel.app

# Java Certificado Service
JAVA_CERTIFICADO_SERVICE_URL=http://localhost:3002

# Configurações de Criptografia
ENCRYPTION_KEY=fenix-encryption-key-2024-super-secure-32bytes

# Configurações da API NFe Externa
NFE_API_URL=http://localhost:8080/api
NFE_API_USERNAME=admin
NFE_API_PASSWORD=admin123
NFE_API_TOKEN=default-token
NFE_WEBHOOK_SECRET=default-webhook-secret
NFE_API_TIMEOUT=30000
NFE_API_RETRY_ATTEMPTS=3
NFE_API_RETRY_DELAY=1000
NFE_SYNC_INTERVAL=120000


# Configurações TypeORM
TYPEORM_HOST=localhost
TYPEORM_PORT=5432
TYPEORM_USERNAME=postgres
TYPEORM_PASSWORD=fenix123
TYPEORM_DATABASE=fenix

# Configurações Redis
REDIS_URL=redis://localhost:6379
