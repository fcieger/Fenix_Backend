# Configurações do Backend FENIX
# Renomeie este arquivo para .env

NODE_ENV=development
PORT=3001

# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=fenix_user
DB_PASSWORD=fenix_password
DB_DATABASE=fenix_db

# Configurações JWT
JWT_SECRET=fenix-jwt-secret-key-2024-super-secure
JWT_EXPIRES_IN=24h

# Configurações CORS
CORS_ORIGIN=http://localhost:3004,http://localhost:3000,http://localhost:8080

